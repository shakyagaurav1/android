<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/avatar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/avatar"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android">
        <activity android:name=".RecyclerView"></activity>
        <activity android:name=".CardViewSpinner" />
        <activity android:name=".ListView" />
        <activity android:name=".listview" />
        <activity android:name=".FrameLayout" />
        <activity android:name=".TableLayout" />
        <activity android:name=".RelativeLayout" />
        <activity android:name=".LinearLayout" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>